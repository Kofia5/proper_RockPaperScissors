<h3>Log Options</h3> 

<nav>
  <ul>
    <% if @current_user %>
      <li><%= link_to 'Play a Game?', new_game_path %></li>
      <li><%= link_to 'Edit Profile', edit_user_path(@current_user) %></li>
      <li><%= link_to 'Logout', logout_path %></li>
    <% else %>
      <li><%= link_to 'Register New User', register_path %></li>
      <li><%= link_to 'Login', login_path %></li>
    <% end %>
  </ul>
</nav>
