<h1>Register</h1>
<%= form_for @user, :url => account_path do |f| %>
  <%= render :partial => 'error', :object => f %>
  <br/>
  <%= f.label :email %><br/>
  <%= f.text_field :email %><br/>  
  <br/>
  <%= render :partial => 'form', :object => f %>
  <%= f.submit "Register" %>
<% end %>

<%= link_to 'Login', new_user_session_path %>
 |
<%= link_to 'Leaderboard', users_path %>
